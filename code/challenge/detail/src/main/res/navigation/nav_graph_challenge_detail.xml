<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_challenge_detail"
    app:startDestination="@id/fragmentChallengeDetail">

    <fragment
        android:id="@+id/fragmentChallengeDetail"
        android:name="com.nicholasrutherford.chal.challenge.detail.ChallengeDetailFragment"
        android:label="challenge_detail_fragment"
        tools:layout="@layout/fragment_challenge_detail">

        <argument
            android:name="challengeTitle"
            app:argType="string"
            />

        <argument
            android:name="challengeCategory"
            app:argType="string"
            />

        <argument
            android:name="challengeUrl"
            app:argType="string"
            />

        <argument
            android:name="challengeDesc"
            app:argType="string"
            />

        <argument
            android:name="challengeTime"
            app:argType="string"
            />

        <argument
            android:name="challengeDuration"
            app:argType="integer"
            />

        <argument
            android:name="challengeCategoryNumber"
            app:argType="integer"
            />

    </fragment>
</navigation>